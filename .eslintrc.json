{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base", "plugin:jsdoc/recommended", "prettier"],
  "plugins": ["jsdoc"],
  "rules": {
    "no-console": "off",
    "import/prefer-default-export": ["error", { "target": "any" }],
    "import/extensions": ["error", { "js": "always" }]
  }
}
